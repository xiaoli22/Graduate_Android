<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CustomImageButton">
        <attr name="imageButtonResource" format="reference" />
        <attr name="hintText" format="string" />
    </declare-styleable>

    <declare-styleable name="CustomMessageBar">
        <attr name="image" format="reference" />
        <attr name="time" format="string" />
        <attr name="date" format="string" />
        <attr name="income" format="string" />
        <attr name="outcome" format="string" />
    </declare-styleable>


    <declare-styleable name="CustomReturn">
        <attr name="title" format="string" />
    </declare-styleable>


    <declare-styleable name="CustomInput">
        <attr name="textColor" format="color"/>
        <attr name="text" format="string" />

    </declare-styleable>

    <declare-styleable name="CustomMessageShow">
        <attr name="titleM" format="string" />
        <attr name="imageM" format="reference" />
        <attr name="numberM" format="string" />
        <attr name="incomeM" format="string" />
        <attr name="outcomeM" format="string" />
    </declare-styleable>

    <declare-styleable name="CustomDisplayCard">
        <attr name="accountD" format="string"/>
        <attr name="assetD" format="string"/>
        <attr name="imageD" format="reference"/>
        <attr name="sourceD" format="string"/>
        <attr name="moneyD" format="string"/>
    </declare-styleable>

    <declare-styleable name="CustomAddNormal">
        <attr name="imageN" format="reference"/>
        <attr name="typeN" format="string"/>
        <attr name="dataN" format="string"/>
        <attr name="types" format="string"/>
    </declare-styleable>

    <declare-styleable name="CustomAddRemark">
        <attr name="imageR" format="reference"/>
        <attr name="typeR" format="string"/>
        <attr name="dataR" format="string"/>
    </declare-styleable>


</resources>
